{
  "name": "theaentmachine/aent-bootstrap",
  "homepage": "https://github.com/theaentmachine/aent-bootstrap",
  "type": "aent",
  "license": "MIT",
  "authors": [
    {
      "name" : "Julien Neuhart",
      "email" : "j.neuhart@thecodingmachine.com",
      "homepage" : "https://github.com/gulien"
    }
  ],
  "require": {
    "php": ">=7.1",
    "theaentmachine/aent-console": "master",
    "thecodingmachine/safe": "^0.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "squizlabs/php_codesniffer": "^3.2",
    "phpstan/phpstan": "^0.10.1",
    "thecodingmachine/phpstan-strict-rules": "^0.10.2",
    "thecodingmachine/phpstan-safe-rule": "^0.1.0@dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gulien/aent-console"
    }
  ],
  "autoload": {
    "psr-4": {
      "TheAentMachine\\AentBootstrap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheAentMachine\\AentBootstrap\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "phpcbf",
    "cs-check": "phpcs",
    "phpstan": "phpstan analyse src -c phpstan.neon --level=7 --no-progress -vvv",
    "phpunit": "phpunit --configuration phpunit.xml.dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
